<Window x:Class="MeshSimplificationTest.SBRepVM.OxyplotVizualizer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MeshSimplificationTest.SBRepVM"
        xmlns:oxy="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Name="viewer"
        Title="OxyplotVizualizer" Height="450" Width="800">
    <Grid>
        <oxy:PlotView Model="{Binding Model, UpdateSourceTrigger=PropertyChanged, ElementName=viewer}"/>
    </Grid>
</Window>
